<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ucsb.integration">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPage.Share.EditListing"></activity>
        <activity
            android:name=".MainPage.Listing.ViewListingActivity"
            android:label="@string/title_activity_view_listing"
            android:parentActivityName=".MainPage.Find.FindActivity"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity android:name=".MainPage.Profile.EditProfileActivity" />
        <activity android:name=".MainPage.Listing.UserhisActivity" />
        <activity android:name=".MainPage.Listing.DirectionActivity" />
        <activity android:name=".MainPage.Listing.CreateListingActivity" />
        <activity
            android:name=".MainPage.Message.MessagePersonActivity"
            android:parentActivityName=".MainPage.Message.MessageActivity" />
        <activity android:name=".MainPage.Profile.SetupActivity" />
        <activity
            android:name=".MainPage.Share.ListHistory"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainPage.ShowList.item" />
        <activity android:name=".MainPage.ShowList.ShowList" />
        <activity
            android:name=".StartActivity"
            android:label="@string/title_activity_start"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainPage.ShowList.ItemDetail"
            android:label="@string/title_activity_start"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainPage.Message.MessageActivity"
            android:label="@string/title_activity_message"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainPage.Profile.ProfileActivity"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainPage.Find.FindActivity"
            android:label="@string/title_activity_find"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login.BasicLoginActivity"
            android:label="@string/title_activity_basic_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>